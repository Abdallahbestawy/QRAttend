<div id="formContainer">
    <form method="post" id="addSectionGroupsForm" enctype="multipart/form-data">
        <div class="card mt-4" style="border: 2px solid #ced4da; border-radius: 0.25rem;">
            <div class="card-header" style="background-color: #f8f9fa; border-bottom: 1px solid #ced4da;">
                <h2 style="margin-bottom: 0;">Add New Students</h2>
            </div>
            <div class="card-body">
                <input type="hidden" id="groupId" name="groupId" value="@ViewBag.GroupId" />
                <div class="form-group">
                    <label for="file">Upload Excel File:</label><br />
                    <input type="file" class="form-control-file" id="file" name="File" accept=".xlsx, .xls, .csv">
                </div>
            </div>
            <div class="card-footer" style="background-color: #f8f9fa; border-top: 1px solid #ced4da;">
                <button type="submit" class="btn btn-primary">Save</button>
            </div>
        </div>
    </form>
</div>
